{
    "apiVersion": "v1",
    "items": [
        {
            "apiVersion": "v1",
            "kind": "Namespace",
            "metadata": {
                "labels": {
                    "name": "hub"
                },
                "name": "hub"
            }
        },
        {
            "apiVersion": "networking.k8s.io/v1beta1",
            "kind": "Ingress",
            "metadata": {
                "name": "hub",
                "namespace": "hub"
            },
            "spec": {
                "rules": [
                    {
                        "host": "hub.local",
                        "http": {
                            "paths": [
                                {
                                    "backend": {
                                        "serviceName": "hub",
                                        "servicePort": 80
                                    }
                                }
                            ]
                        }
                    }
                ]
            }
        },
        {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
                "name": "hub-web",
                "namespace": "hub"
            },
            "spec": {
                "ports": [
                    {
                        "port": 8000,
                        "protocol": "TCP"
                    }
                ],
                "selector": {
                    "service": "hub-web"
                }
            }
        }
    ],
    "kind": "List"
}
